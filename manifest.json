{
    "manifest_version":3,
    "name":"tabs",
    "description":"close duplicate tabs",
    "version":"1.0",
    "action":{
        "default_icon":"./images/icon-32.png"
    },
    "permissions":["tabs","activeTab","scripting"],
    "background":{
        "service_worker":"background.js"
    },
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+I",
            "mac": "Command+I"
          }
        }
      }
}